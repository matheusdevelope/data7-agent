{
  "name": "data7-agent",
  "version": "1.0.0",
  "description": "",
  "main": "./dist",
  "scripts": {
    "build:app": "npx tsc",
    "build:dev:app": "npx tsc -w",
    "build:front:for:electron": "tsc -p front/tsconfig.json && cd front && cross-env ELECTRON=true vite build",
    "build:dev:front:for:electron": "tsc -p front/tsconfig.json && cd front && cross-env ELECTRON=true vite build --watch",
    "build": "concurrently --raw \"npm run build:app\"  \"npm run build:front:for:electron\"",
    "build:dev": "concurrently --raw  \"npm run build:dev:front:for:electron\" \"npm run build:dev:app\"",
    "dev:vite": "cd front && vite ",
    "dev:server": "concurrently \"npm run dev:vite\" \"npm run build:dev:app\"",
    "dev": "npm run build:front:for:electron && cross-env IS_DEV=true electron ./dist",
    "start": "concurrently --raw \"npm run build:front:for:electron\" \"npm run build:app\" \"electron ./dist\""
  },
  "keywords": [],
  "author": "Matheus Teixeira",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "currency-formatter": "^1.5.9",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "electron": "^19.0.7",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "firebase-admin": "^11.0.0",
    "gn-api-sdk-typescript": "^1.0.7",
    "moment": "^2.29.3",
    "node-localstorage": "^2.2.1",
    "qrcode": "^1.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "@firebase/firestore-types": "^2.5.0",
    "@types/cors": "^2.8.12",
    "@types/currency-formatter": "^1.5.1",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.1",
    "@types/node-localstorage": "^1.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.9"
  }
}
